{
  "name": "assetmanager",
  "version": "1.0.0",
  "scripts": {
    "build": "browserify flowers_oop.mjs -t [ babelify --presets [@babel/preset-env] --global ] --outfile bundle.js"
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "three": "^0.162.0",
    "debug": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babelify": "^10.0.0"
  },
  "browserify": {
    "transform": [
      ["babelify", {
        "presets": ["@babel/preset-env"],
        "global": true
      }]
    ]
  }
}
